# Local SQLite API (run `npm run server`).
VITE_SQLITE_API_URL=http://localhost:4000

# Set to true to enable Supabase backend instead of SQLite API.
VITE_USE_SUPABASE=false

# Supabase Configuration (optional, legacy mode)
VITE_SUPABASE_PROJECT_ID=your_project_id
VITE_SUPABASE_PUBLISHABLE_KEY=your_publishable_key
VITE_SUPABASE_URL=https://your_project.supabase.co

# AI Chat Assistant Provider
AI_PROVIDER=anthropic  # Options: anthropic (default), gemini

# Gemini API key (used by npm run server to call Gemini Flash)
GEMINI_API_KEY=

# Anthropic API key (used by npm run server to call Claude Sonnet 4.5)
ANTHROPIC_API_KEY=

# AI() SQL Function Provider
AI_FUNCTION_PROVIDER=openai  # Options: openai (default), gemini
AI_FUNCTION_MODEL=  # Optional model override (defaults: gpt-4o-mini for OpenAI, gemini-2.0-flash-exp for Gemini)

# OpenAI API Key (required for AI() SQL function with OpenAI provider)
OPENAI_API_KEY=
